    "@type" : "owl:Class",
    "comment" : "A linear description element in a drawing.",
    "label" : "Line",
    "subClassOf" : "dano:DescriptionElement"
  }, {
    "@id" : "dano:SectionSymbol",
    "@type" : "owl:Class",
