    "subClassOf" : "owl:Thing"
  }, {
    "@id" : "dano:DrawingElementMeta",
    "@type" : "owl:Class",
    "comment" : "Metadata for drawing elements, including inference confidence and methods used.",
    "label" : "Drawing Element Meta",
