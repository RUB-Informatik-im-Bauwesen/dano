    "domain" : "dano:DrawingElementMeta",
    "label" : "inferred at",
    "range" : "xsd:date"
  }, {
    "@id" : "dano:inferredBy",
    "@type" : "owl:DatatypeProperty",
    "comment" : "Indicates the actor, company, or software that inferred the drawing element.",
    "domain" : "dano:DrawingElementMeta",
    "label" : "inferred by",
    "range" : "xsd:string"
  }, {
    "@id" : "dano:inferredFrom",
